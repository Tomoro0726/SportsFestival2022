        <th>4組-半減</th>
        <th>5組-半減</th>
        <th>6組-半減</th>
        <th>7組-半減</th>
        <th>8組-半減</th>

      </tr>
      <tr>
        <th><select class="input select" name="oneh">
            <% if(items.oneh=="半減"){ %>
            <% var gen="selected" %>
            <%}else if(items.hyouka==""){%>
            <% var ngen="selected" %>

            <% }; %>
            <option<%= gen %>>
              </option>
              <option<%= ngen %>>半減</option>

          </select></th>
        <th><select class="input select" name="twoh">

            <option></option>
            <option>半減</option>

          </select></th>
        <th><select class="input select" name="threeh">

            <option></option>
            <option>半減</option>

          </select></th>
        <th><select class="input select" name="fourh">

            <option></option>
            <option>半減</option>

          </select></th>
        <th><select class="input select" name="fiveh">

            <option></option>
            <option>半減</option>

          </select></th>
        <th><select class="input select" name="sixh">

            <option></option>
            <option>半減</option>

          </select></th>
        <th><select class="input select" name="sevenh">

            <option></option>
            <option>半減</option>

          </select></th>
        <th><select class="input select" name="eighth">

            <option></option>
            <option>半減</option>

          </select></th>
      </tr>
    </table>
    <table>
      <tr>
        <th>1組-備考</th>
        <th>2組-備考</th>
        <th>3組-備考</th>
        <th>4組-備考</th>


      </tr>
      <tr>
        <th><textarea class="bikou" name="oneb"></textarea></th>
        <th><textarea class="bikou" name="twob"></textarea></th>
        <th><textarea class="bikou" name="threeb"></textarea></th>
